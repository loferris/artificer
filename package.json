{
  "name": "ai-workflow-engine",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:standalone": "tsx src/server/standalone.ts",
    "build": "prisma generate && next build",
    "start": "next start",
    "start:standalone": "NODE_ENV=production tsx src/server/standalone.ts",
    "lint": "next lint",
    "format": "prettier --write .",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "COVERAGE=true NODE_OPTIONS='--max-old-space-size=4096' vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "test:e2e:run": "vitest run --config vitest.e2e.config.ts",
    "test:e2e:ui": "vitest --ui --config vitest.e2e.config.ts",
    "postinstall": "prisma generate",
    "db:up": "docker compose up postgres -d",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up postgres -d",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:admin": "docker compose --profile admin up pgadmin -d",
    "db:logs": "docker compose logs -f postgres",
    "db:seed": "node --env-file=.env --import tsx scripts/seed-vector-data.ts",
    "db:seed:no-embeddings": "SKIP_EMBEDDINGS=true node --env-file=.env --import tsx scripts/seed-vector-data.ts",
    "generate-api-key": "node --env-file=.env --import tsx scripts/generate-api-key.ts",
    "list-api-keys": "node --env-file=.env --import tsx scripts/list-api-keys.ts",
    "revoke-api-key": "node --env-file=.env --import tsx scripts/revoke-api-key.ts",
    "highlight:up": "docker compose -f docker-compose.highlight.yml up -d",
    "highlight:down": "docker compose -f docker-compose.highlight.yml down",
    "highlight:logs": "docker compose -f docker-compose.highlight.yml logs -f",
    "highlight:restart": "docker compose -f docker-compose.highlight.yml restart",
    "highlight:clean": "docker compose -f docker-compose.highlight.yml down -v",
    "dev:with-highlight": "npm run highlight:up && npm run dev"
  },
  "dependencies": {
    "@axiomhq/js": "^1.3.1",
    "@highlight-run/next": "^7.9.38",
    "@highlight-run/node": "^3.12.22",
    "@portabletext/types": "^3.0.0",
    "@prisma/client": "^6.15.0",
    "@tanstack/react-query": "^5.0.0",
    "@trpc/client": "^11.5.1",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.5.1",
    "@types/cors": "^2.8.19",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.17",
    "autoprefixer": "^10.4.21",
    "chromadb": "^3.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "diff": "^8.0.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.3",
    "js-yaml": "^4.1.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-frontmatter": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.2",
    "next": "^15.5.2",
    "openai": "^6.8.1",
    "pdf2pic": "^3.2.0",
    "pg": "^8.16.3",
    "pino": "^9.2.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "superjson": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tiktoken": "^1.0.22",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/diff": "^7.0.2",
    "@types/pg": "^8.15.5",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.11.1",
    "msw-trpc": "^2.0.1",
    "node-mocks-http": "^1.17.2",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.2",
    "prisma": "^6.15.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  }
}
