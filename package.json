{
  "name": "artificer-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Artificer monorepo - UI libraries and applications",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter @artificer/workbench dev",
    "dev:translator": "pnpm --filter @artificer/translator-demo dev",
    "dev:worldbuilder": "pnpm --filter @artificer/worldbuilder-demo dev",
    "build": "pnpm -r build",
    "build:workbench": "pnpm --filter @artificer/workbench build",
    "build:translator": "pnpm --filter @artificer/translator-demo build",
    "build:worldbuilder": "pnpm --filter @artificer/worldbuilder-demo build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "clean": "pnpm -r exec rm -rf node_modules .next dist",
    "format": "prettier --write .",
    "postinstall": "pnpm --filter @artificer/workbench postinstall",
    "db:up": "docker compose up postgres -d",
    "db:down": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up postgres -d",
    "db:migrate": "cd apps/workbench && prisma migrate dev",
    "db:studio": "cd apps/workbench && prisma studio",
    "db:admin": "docker compose --profile admin up pgadmin -d",
    "db:logs": "docker compose logs -f postgres",
    "db:seed": "cd apps/workbench && node --env-file=../../.env --import tsx scripts/seed-vector-data.ts",
    "db:seed:no-embeddings": "cd apps/workbench && SKIP_EMBEDDINGS=true node --env-file=../../.env --import tsx scripts/seed-vector-data.ts",
    "generate-api-key": "cd apps/workbench && node --env-file=../../.env --import tsx scripts/generate-api-key.ts",
    "list-api-keys": "cd apps/workbench && node --env-file=../../.env --import tsx scripts/list-api-keys.ts",
    "revoke-api-key": "cd apps/workbench && node --env-file=../../.env --import tsx scripts/revoke-api-key.ts",
    "highlight:up": "docker compose -f docker-compose.highlight.yml up -d",
    "highlight:down": "docker compose -f docker-compose.highlight.yml down",
    "highlight:logs": "docker compose -f docker-compose.highlight.yml logs -f",
    "highlight:restart": "docker compose -f docker-compose.highlight.yml restart",
    "highlight:clean": "docker compose -f docker-compose.highlight.yml down -v",
    "dev:with-highlight": "pnpm highlight:up && pnpm dev"
  },
  "dependencies": {
    "@axiomhq/js": "^1.3.1",
    "@highlight-run/next": "^7.9.38",
    "@highlight-run/node": "^3.12.22",
    "@portabletext/types": "^3.0.0",
    "@prisma/client": "^6.15.0",
    "@tanstack/react-query": "^5.0.0",
    "@trpc/client": "^11.5.1",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.5.1",
    "@types/cors": "^2.8.19",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.17",
    "autoprefixer": "^10.4.21",
    "chromadb": "^3.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "diff": "^8.0.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.5.3",
    "js-yaml": "^4.1.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-frontmatter": "^2.0.1",
    "mdast-util-to-markdown": "^2.1.2",
    "next": "^15.5.2",
    "openai": "^6.8.1",
    "pdf-parse": "^2.4.5",
    "pdf2pic": "^3.2.0",
    "pg": "^8.16.3",
    "pino": "^9.2.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark": "^15.0.1",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "superjson": "^2.2.0",
    "tailwindcss": "^3.4.0",
    "tiktoken": "^1.0.22",
    "unified": "^11.0.5",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "husky": "^9.0.11",
    "is-ci": "^3.0.1",
    "lint-staged": "^15.2.2",
    "prettier": "^3.3.2",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
